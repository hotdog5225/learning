    "import concurrent.futures\n",
    "\n",
    "'''\n",
    "线程池\n",
    "'''\n",
    "\n",
    "num_list = [1,2,3]\n",
    "# 构建线程池实例\n",
    "with concurrent.futures.ThreadPoolExecutor() as executor:\n",
    "    # 多线程, 每个线程将一个num --> num*num\n",
    "    result = executor.map(lambda x: x*x, num_list) # generator\n",
    "    result = list(result)\n",
    "    print(result) # [1, 4, 9]"
   ]
